{
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "titleBar.activeBackground": "#a58210",
    "titleBar.inactiveBackground": "#a5821099",
    "activityBar.activeBackground": "#d3a715",
    "activityBar.activeBorder": "#e2fcf6",
    "activityBar.background": "#d3a715",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e2fcf6",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#d3a715",
    "statusBar.background": "#a58210",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#d3a715",
    "statusBarItem.remoteBackground": "#a58210",
    "statusBarItem.remoteForeground": "#e7e7e7"
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "prettier.disableLanguages": ["javascript", "javascriptreact"],
  "peacock.color": "#a58210"
}
